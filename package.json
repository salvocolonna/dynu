    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.9.0",
    "@types/cors": "2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/moment": "2.13.0",
    "@types/node": "^14.0.1",
    "nodemon": "^2.0.4",
